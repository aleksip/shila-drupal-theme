{% if attributes is not defined %}
  {% set attributes = create_attribute({'class': ['region', 'region-secondary-menu']}) %}
{% endif %}
{% if content is not defined %}
  {% set content %}
    {%
      include '@organisms/block/classy-menu-block/demo/classy-menu-block-demo.twig' with {
        'title_attributes': create_attribute({'class': ['visually-hidden']})
      } only
    %}
  {% endset %}
{% endif %}

{%
  include '@organisms/layout/classy-region/demo/classy-region-demo.twig' with {
    'attributes': attributes,
    'content': content
  } only
%}
