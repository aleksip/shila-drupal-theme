{% if attributes is not defined %}
  {% set attributes = create_attribute({'class': ['region', 'region-header']}) %}
{% endif %}
{% if content is not defined %}
  {% set content %}
    {%
      include '@organisms/block/classy-branding-block/demo/classy-branding-block-demo.twig' with {
        'site_slogan': '',
      } only
    %}
  {% endset %}
{% endif %}

{%
  include '@organisms/layout/classy-region/demo/classy-region-demo.twig' with {
    'attributes': attributes,
    'content': content
  } only
%}
